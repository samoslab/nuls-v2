[global]
encoding=UTF-8
language=en
logPath=./Logs
logLevel=INFO
dataPath=./data
#默认本链ID
chainId=2
#默认本链主资产ID
assetId=1
#默认链名称
chainName=nuls2
#本链默认资产符号
symbol=NULS
#默认资产的小数精确位数
decimals=8
#主网链id
mainChainId=2
#主网主资产id
mainAssetId=1
#主网主资产符号
mainSymbol=NULS
blackHolePublicKey=0298f88c3cae67385ce3cbee00f78816db3e56e566b62bd0f4c5b45f205d3021c3

[network]
port=18001
crossPort=18002
#魔法参数
packetMagic=20201113
#种子节点
selfSeedIps=192.168.1.131:18001,192.168.1.132:18001,192.168.1.133:18001,192.168.1.134:18001,192.168.1.37:18001
#最大入网连接数
maxInCount=100
#最大出网连接数
maxOutCount=20

[account]
keystoreFolder=/keystore/backup

[block]
#区块运行时参数
#分叉链监视线程运行间隔
forkChainsMonitorInterval=10000
#孤儿链监视线程运行间隔
orphanChainsMonitorInterval=10000
#孤儿链维护线程运行间隔
orphanChainsMaintainerInterval=5000
#缓存数据库容量监视线程运行间隔
storageSizeMonitorInterval=30000
#网络监视线程运行间隔
networkResetMonitorInterval=180000
#节点数量监视线程运行间隔
nodesMonitorInterval=5000
#TxGroup获取线程运行间隔
txGroupRequestorInterval=1000
#TxGroup任务启动延时
txGroupTaskDelay=3000
#区块最大字节数
blockMaxSize=5242880
#区块扩展字段最大字节数
extendMaxSize=1024
#本地区块高度不更新时,引发重置网络动作的时间间隔
resetTime=180000
#引发分叉链切换的高度差阈值
chainSwtichThreshold=3
#分叉链、孤儿链区块最大缓存数量
cacheSize=1000
#接收新区块的范围
heightRange=1000
#批量下载区块时,如果收到CompleteMessage时,区块还没有保存完,每一个区块预留多长等待时间
waitInterval=50
#本地区块与网络区块不一致时,本地最大回滚数
maxRollback=1000
#统计网络上的节点最新区块高度、hash一致的百分比阈值
consistencyNodePercent=60
#最小链接节点数,当链接到的网络节点低于此参数时,会持续等待
minNodeAmount=2
#区块同步过程中,每次从网络上节点下载的区块数量
downloadNumber=10
#为阻止恶意节点提前出块,设置此参数,区块时间戳大于当前时间多少就丢弃该区块
validBlockInterval=60000
#系统正常运行时最多缓存多少个从别的节点接收到的小区块
smallBlockCache=10
#孤儿链维护失败时,年龄加一,此参数是孤儿链能达到的最大年龄,高于这个值会被清理线程清理
orphanChainMaxAge=5
#从网络节点下载单个区块的超时时间
singleDownloadTimeout=10000
#从网络节点下载多个区块的超时时间
batchDownloadTimeout=30000
#批量下载区块时,如果收到CompleteMessage时,区块还没有保存完,最多循环等待几个回合
maxLoop=20
#两次区块同步之间的时间间隔
synSleepInterval=1000
#等待网络稳定的时间间隔
waitNetworkInterval=5000
#分叉链、孤儿链清理时每次清理几分之一
cleanParam=2
#区块同步过程中缓存的区块字节数上限
cachedBlockSizeLimit=26214400


[consensus]
seedNodes=tNULSeBaMkrt4z9FYEkkR9D6choPVvQr94oYZp,tNULSeBaMoGr2RkLZPfJeS5dFzZeNj1oXmaYNe,tNULSeBaMnG8hGcyeygVeyaL5cXV38bQm1rw9M,tNULSeBaMtFXBLB35WSAnoN9ezGLLCbh7sPNNN
password=nuls123456
packingInterval=10
agentAssetId=1
agentChainId=2
awardAssetId=1
feeUnit=100000

[smart_contract]
#合约视图方法调用最大消耗的Gas
maxViewGas=100000000

[api-module]
#api-module模块对外的rpc端口号
rpcPort=18003
#数据库url地址
databaseUrl=127.0.0.1
#数据库端口号
databasePort=27017
#连接池最大数
maxAliveConnect=20
#连接最大等待时间
maxWaitTime=120000
#连接超时时间
connectTimeOut=30000

[cross-chain]
minNodeAmount=8
maxNodeAmount=10
sendHeight=5
byzantineRatio=50
crossSeedIps=192.168.1.131:18002

[protocol-update]
interval=5